using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


class Person
{
    string name;
    int age;

    public Person()
    {
        name = "Unknown";
        age = 18;
    }

    public Person(string n, int a)
    {
        name = n;
        age = a;
    }

    public void Show() => Console.WriteLine(name + " " + age);
}

class Room
{
    int number;
    string type;

    public Room()
    {
        number = 0;
        type = "Normal";
    }

    public Room(int n, string t)
    {
        number = n;
        type = t;
    }

    public void Show() => Console.WriteLine(number + " " + type);
}

class Watch
{
    string brand;
    int price;

    public Watch()
    {
        brand = "Local";
        price = 500;
    }

    public Watch(string b, int p)
    {
        brand = b;
        price = p;
    }

    public void Show() => Console.WriteLine(brand + " " + price);
}

class Student
{
    int id;
    string name;

    public Student()
    {
        id = 0;
        name = "NA";
    }

    public Student(int i, string n)
    {
        id = i;
        name = n;
    }

    public void Show() => Console.WriteLine(id + " " + name);
}

class Book
{
    string title;
    int pages;

    public Book()
    {
        title = "Unknown";
        pages = 100;
    }

    public Book(string t, int p)
    {
        title = t;
        pages = p;
    }

    public void Show() => Console.WriteLine(title + " " + pages);
}

class Account
{
    int accNo;
    decimal balance;

    public Account()
    {
        accNo = 0;
        balance = 0;
    }

    public Account(int a, decimal b)
    {
        accNo = a;
        balance = b;
    }

    public void Show() => Console.WriteLine(accNo + " " + balance);
}

class Mobile
{
    string model;
    int price;

    public Mobile()
    {
        model = "Basic";
        price = 10000;
    }

    public Mobile(string m, int p)
    {
        model = m;
        price = p;
    }

    public void Show() => Console.WriteLine(model + " " + price);
}

class Laptop
{
    string brand;
    int ram;

    public Laptop()
    {
        brand = "Generic";
        ram = 8;
    }

    public Laptop(string b, int r)
    {
        brand = b;
        ram = r;
    }

    public void Show() => Console.WriteLine(brand + " " + ram);
}

class Circle
{
    double radius;

    public Circle()
    {
        radius = 1;
    }

    public Circle(double r)
    {
        radius = r;
    }

    public void Show() => Console.WriteLine(radius);
}

class Movie
{
    string name;
    string language;

    public Movie()
    {
        name = "NA";
        language = "Hindi";
    }

    public Movie(string n, string l)
    {
        name = n;
        language = l;
    }

    public void Show() => Console.WriteLine(name + " " + language);
}

class Country
{
    string name;
    string capital;

    public Country()
    {
        name = "India";
        capital = "Delhi";
    }

    public Country(string n, string c)
    {
        name = n;
        capital = c;
    }

    public void Show() => Console.WriteLine(name + " " + capital);
}

class Company
{
    string name;
    int employees;

    public Company()
    {
        name = "Startup";
        employees = 10;
    }

    public Company(string n, int e)
    {
        name = n;
        employees = e;
    }

    public void Show() => Console.WriteLine(name + " " + employees);
}

class Bike
{
    string model;
    int mileage;

    public Bike()
    {
        model = "Standard";
        mileage = 40;
    }

    public Bike(string m, int mi)
    {
        model = m;
        mileage = mi;
    }

    public void Show() => Console.WriteLine(model + " " + mileage);
}

class Program
{
    static void Main()
    {
        new Person().Show();
        new Room(101, "AC").Show();
        new Watch("Casio", 2000).Show();
        new Student(1, "Aditya").Show();
        new Book("CSharp", 300).Show();
        new Account(123, 50000).Show();
        new Mobile("iPhone", 60000).Show();
        new Laptop("Dell", 16).Show();
        new Circle(5).Show();
        new Movie("3 Idiots", "Hindi").Show();
        new Country("India", "Delhi").Show();
        new Company("TCS", 50000).Show();
        new Bike("Pulsar", 45).Show();
    }
}
